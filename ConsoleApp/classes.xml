      <Attribute Type="CommentAttribute">
        <Comment>класс Pig — всёядная</Comment>
      </Attribute>
    </Attributes>
    <Methods>
      <Method Name="GetFavouriteFood" ReturnType="eFavoriteFood" />
      <Method Name="SayHello" ReturnType="Void" />
    </Methods>
  </Type>
</Library>