        <Comment>базовый абстрактный класс Animal</Comment>
      </Attribute>
    </Attributes>
    <Properties>
      <Property Name="Country" Type="String" />
      <Property Name="Name" Type="String" />
      <Property Name="HideFromOtherAnimals" Type="Boolean" />
      <Property Name="Classification" Type="eClassificationAnimal" />
    </Properties>
    <Methods>
      <Method Name="Deconstruct" ReturnType="Void">
        <Parameters>
          <Parameter Name="name" Type="String&amp;" />
          <Parameter Name="country" Type="String&amp;" />
        </Parameters>
      </Method>
      <Method Name="GetClassificationAnimal" ReturnType="eClassificationAnimal" />
      <Method Name="GetFavouriteFood" ReturnType="eFavoriteFood" />
      <Method Name="SayHello" ReturnType="Void" />
    </Methods>
  </Type>
  <Type Name="CommentAttribute" Namespace="MyClassLibrary" Kind="Class">
    <BaseType>Attribute</BaseType>
    <Attributes>
      <Attribute Type="NullableContextAttribute" />
      <Attribute Type="NullableAttribute" />
      <Attribute Type="AttributeUsageAttribute" />
    </Attributes>
    <Properties>
      <Property Name="Comment" Type="String" />
    </Properties>
  </Type>
