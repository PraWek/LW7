    <BaseType>Animal</BaseType>
    <Attributes>
      <Attribute Type="CommentAttribute">
        <Comment>класс Cow — даёт молоко</Comment>
      </Attribute>
    </Attributes>
    <Methods>
      <Method Name="GetFavouriteFood" ReturnType="eFavoriteFood" />
      <Method Name="SayHello" ReturnType="Void" />
    </Methods>
  </Type>
  <Type Name="eClassificationAnimal" Namespace="MyClassLibrary" Kind="Enum">
    <BaseType>Enum</BaseType>
  </Type>
  <Type Name="eFavoriteFood" Namespace="MyClassLibrary" Kind="Enum">
    <BaseType>Enum</BaseType>
  </Type>
  <Type Name="Lion" Namespace="MyClassLibrary" Kind="Class">
    <BaseType>Animal</BaseType>
    <Attributes>
      <Attribute Type="CommentAttribute">
        <Comment>класс Lion - царь зверей</Comment>
      </Attribute>
    </Attributes>
    <Methods>
      <Method Name="GetFavouriteFood" ReturnType="eFavoriteFood" />
      <Method Name="SayHello" ReturnType="Void" />
    </Methods>
  </Type>
  <Type Name="Pig" Namespace="MyClassLibrary" Kind="Class">
